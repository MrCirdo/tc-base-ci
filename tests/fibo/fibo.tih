function fibo(n : int) : int = (
    let
        var a := 0
        var b := 1
    in
        if n < 0 then
            -1
        else
            (
                for k := 1 to n do
                    let
                        var t := a
                    in
                        (
                            a := b;
                            b := b + t
                        )
                    end;
                a
            )
    end
)